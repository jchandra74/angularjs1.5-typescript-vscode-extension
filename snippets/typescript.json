{
    "AngularJS Module": {
        "prefix": "ajs-module",
        "description": "AngularJS module",
        "body": [
            "import * as angular from \"angular\";",
            "import { IModule } from \"angular\";",
            "import \"angular-sanitize\";",
            "import \"angular-animate\";",
            "",
            "// import the pre gulped template cache...",
            "import \"./tmp/templates\";",
            "",
            "// add component, services, etc. import here...",
            "import { ${ComponentName}Component } from \"./component/${ComponentName}/${ComponentName}.component\";",
            "",
            "// add css, and other misc import here...",
            "// remove if not needed",
            "import \"./module.css!\";",
            "",
            "let app: IModule = angular.module(\"${ModuleId}\", [",
            "\t\"ngSanitize\",",
            "\t\"ngAnimate\"",
            "]);",
            "",
            "// add module registration here...",
            "app",
            "\t.component(${ComponentName}Component.selector, ${ComponentName}Component)$0;",
            "",
            "// Uncomment if bootstrapping is needed or remove if not...",
            "//let appRootElement: Element = document.body;",
            "//angular.bootstrap(appRootElement, [ \"${ModuleId}\" ]);"
        ]
    },
    "AngularJS Component": {
        "prefix": "ajs-component",
        "description": "AngularJS component",
        "body": [
            "import { ILogService } from \"angular\";",
            "",
            "class ${Component}ComponentController {",
            "",
            "\tComponentName: string = \"${Component}Component\"",
            "",
            "\tstatic \\$inject: string[] = [\"\\$log\"];",
            "\tconstructor(",
            "\t\tprivate \\$log: ILogService",
            "\t) { }",
            "",
            "\t\\$onInit(): void {",
            "\t\tthis.\\$log.info(`[\\${this.ComponentName}] onInit`);",
            "\t\t$0",
            "\t}",
            "",
            "}",
            "",
            "const ${Component}Component: any = {",
            "\tselector: \"${Selector}\",",
            "\tcontroller: ${Component}ComponentController,",
            "templateUrl: \"\"",
            "};",
            "",
            "export { ${Component}Component };"
        ]
    },
    "AngularJS Service": {
        "prefix": "ajs-service",
        "description": "AngularJS Service",
        "body": [
            "import { ILogService } from \"angular\";",
            "",
            "class ${Name}Service {",
            "",
            "\tstatic Name: string = \"${Name}Service\";",
            "\tstatic \\$inject: string[] = [ \"\\$log\" ];",
            "\tconstructor (",
            "\t\tprivate \\$log: ILogService",
            "\t) { }",
            "",
            "\t${MethodName}(${Parameter}): ${ReturnType} {",
            "\t\tthis.\\$log.info(`[\\${${Name}Service.Name}] ${MethodName}`);",
            "\t\t$0",
            "\t}",
            "}",
            "",
            "export { ${Name}Service }"
        ]
    }
}